		</dependency>
	</dependencies>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<SpringFrameworkVersion>5.2.3.RELEASE</SpringFrameworkVersion>
	</properties>
</project>
