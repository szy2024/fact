<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC
        "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.3.0//EN"
        "http://jakarta.apache.org/commons/dtds/validator_1_3_0.dtd">

<form-validation>
    <formset>
        <form name="Md011_01UpdateController">
            <field property="drpID_ShiteiKofuYear" depends="maxlength">
                <msg name="maxlength" key="errors.maxlength"/>
                <arg position="0" name="maxlength" key="drpID_ShiteiKofuYear" />
                <arg position="1" name="maxlength" key="${var:maxlength}" resource="false" />
                <var>
                    <var-name>maxlength</var-name>
                    <var-value>3</var-value>
                </var>

            </field>
            <field property="drpID_ShiteiKofuMonth" depends="maxlength">
                <msg name="maxlength" key="errors.maxlength"/>
                <arg position="0" name="maxlength" key="drpID_ShiteiKofuMonth" />
                <arg position="1" name="maxlength" key="${var:maxlength}" resource="false" />
                <var>
                    <var-name>maxlength</var-name>
                    <var-value>2</var-value>
                </var>

            </field>
            <field property="drpID_ShiteiKofuDate" depends="maxlength">
                <msg name="maxlength" key="errors.maxlength"/>
                <arg position="0" name="maxlength" key="drpID_ShiteiKofuDate" />
                <arg position="1" name="maxlength" key="${var:maxlength}" resource="false" />
                <var>
                    <var-name>maxlength</var-name>
                    <var-value>2</var-value>
                </var>

            </field>

        </form>

    </formset>
</form-validation>